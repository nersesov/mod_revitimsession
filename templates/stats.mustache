                                    <span class="history-percentage-text">{{last_3_percentage}}%</span>
                            </div>
                            {{/last_3_answered}}
                            {{^last_3_answered}}
                                -
                            {{/last_3_answered}}
                        </div>
                        <div class="data-cell">
                            {{#answered_count}}
                                {{correct_count}} | {{answered_count}}
                                <div class="history-percentage-cell">
                                    <div class="history-donut-chart {{percentage_class}}" data-percentage="{{percentage}}"></div>
                                    <span class="history-percentage-text">{{percentage}}%</span>
                    </div>
                            {{/answered_count}}
                            {{^answered_count}}
                                -
                            {{/answered_count}}
                        </div>
                    </div>
                {{/is_root}}
                {{^is_root}}
                    <!-- Parent category with subcategories -->
                    <div class="category-row">
                        <div class="category-cell">
                            <span class="expand-icon" id="exam-icon-{{parent_id}}" data-action="toggle-exam-subcategories" data-parent-id="{{parent_id}}">+</span>
                            <span class="category-name"><strong>{{parentname}}</strong></span>
                        </div>
                        <div class="data-cell">{{parent_total}}</div>
                        <div class="data-cell">
                            {{#parent_most_recent_answered_total}}
                                {{parent_most_recent_correct_total}} | {{parent_most_recent_answered_total}}
                                <div class="history-percentage-cell">
                                    <div class="history-donut-chart {{parent_most_recent_percentage_class}}" data-percentage="{{parent_most_recent_percentage}}"></div>
                                    <span class="history-percentage-text">{{parent_most_recent_percentage}}%</span>
                </div>
                            {{/parent_most_recent_answered_total}}
                            {{^parent_most_recent_answered_total}}
                                -
                            {{/parent_most_recent_answered_total}}
                        </div>
                        <div class="data-cell">
                            {{#parent_last_3_answered_total}}
                                {{parent_last_3_correct_total}} | {{parent_last_3_answered_total}}
                                <div class="history-percentage-cell">
                                    <div class="history-donut-chart {{parent_last_3_percentage_class}}" data-percentage="{{parent_last_3_percentage}}"></div>
                                    <span class="history-percentage-text">{{parent_last_3_percentage}}%</span>
                                </div>
                            {{/parent_last_3_answered_total}}
                            {{^parent_last_3_answered_total}}
                                -
                            {{/parent_last_3_answered_total}}
                                    </div>
                        <div class="data-cell">
                            {{#parent_answered_total}}
                                {{parent_correct_total}} | {{parent_answered_total}}
                                <div class="history-percentage-cell">
                                    <div class="history-donut-chart {{parent_percentage_class}}" data-percentage="{{parent_percentage}}"></div>
                                    <span class="history-percentage-text">{{parent_percentage}}%</span>
                                </div>
                            {{/parent_answered_total}}
                            {{^parent_answered_total}}
                                -
                            {{/parent_answered_total}}
                        </div>
                    </div>
                    <div class="subcategory-container stats-subcategory-container" id="exam-sub-{{parent_id}}">
                        {{#categories}}
                        <div class="subcategory-row">
                            <div class="subcategory-cell">
                                <span class="subcategory-name">{{categoryname}}</span>
                            </div>
                            <div class="data-cell">{{item_count}}</div>
                            <div class="data-cell">
                                {{#most_recent_answered}}
                                    {{most_recent_correct}} | {{most_recent_answered}}
                                    <div class="history-percentage-cell">
                                        <div class="history-donut-chart {{most_recent_percentage_class}}" data-percentage="{{most_recent_percentage}}"></div>
                                        <span class="history-percentage-text">{{most_recent_percentage}}%</span>
                                    </div>
                                {{/most_recent_answered}}
                                {{^most_recent_answered}}
                                    -
                                {{/most_recent_answered}}
                                </div>
                            <div class="data-cell">
                                {{#last_3_answered}}
                                    {{last_3_correct}} | {{last_3_answered}}
                                    <div class="history-percentage-cell">
                                        <div class="history-donut-chart {{last_3_percentage_class}}" data-percentage="{{last_3_percentage}}"></div>
                                        <span class="history-percentage-text">{{last_3_percentage}}%</span>
                                    </div>
                                {{/last_3_answered}}
                                {{^last_3_answered}}
                                    -
                                {{/last_3_answered}}
                            </div>
                            <div class="data-cell">
                            {{#answered_count}}
                                {{correct_count}} | {{answered_count}}
                                <div class="history-percentage-cell">
                                    <div class="history-donut-chart {{percentage_class}}" data-percentage="{{percentage}}"></div>
                                    <span class="history-percentage-text">{{percentage}}%</span>
                                </div>
                            {{/answered_count}}
                            {{^answered_count}}
                                -
                            {{/answered_count}}
                            </div>
                        </div>
                        {{/categories}}
                    </div>
                {{/is_root}}
                {{/exam_categories}}
            </div>
        </div>
    </div>
    
    <!-- History Tab -->
    <div id="history-panel" class="tab-panel" role="tabpanel" aria-labelledby="history">
        <h2>{{session_history}}</h2>
        
        <!-- History Filters -->
        <div class="history-filters">
            <span class="filter-label">{{filters}}</span>
            <label class="filter-checkbox">
                <input type="checkbox" id="show-last-3">
                {{show_only_last_3}}
            </label>
            <label class="filter-checkbox">
                <input type="checkbox" id="study-sessions-only">
                {{view_only_study_sessions}}
            </label>
            <label class="filter-checkbox">
                <input type="checkbox" id="practice-exams-only">
                {{view_only_practice_exams}}
            </label>
        </div>
        
        <!-- History Table -->
        <div class="history-table-wrapper">
            <table class="history-table">
                <thead>
                    <tr>
                        <th>{{session_type}}</th>
                        <th>{{related_study_units}}</th>
                        <th>{{completion_date}}</th>
                        <th>{{total_qs}}</th>
                        <th>{{avg_time_per_question_header}}</th>
                        <th>{{time_of_session}}</th>
                        <th>{{questions_answered_header}}</th>
                        <th>{{questions_correct_header}}</th>
                        <th>{{percent_correct}}</th>
                    </tr>
                </thead>
                <tbody>
                    {{#history_data}}
                    <tr class="history-row" data-session-type="{{exam_type}}" data-is-study="{{is_study_session}}">
                        <td>
                            <div class="session-type-cell">
                                <span class="session-type-text">{{exam_type}}</span>
                                <a href="{{perform_url}}?id={{id}}&examid={{exam_id}}" class="review-btn-hover">{{review}}</a>
                            </div>
                        </td>
                        <td>{{studyunit}}</td>
                        <td>{{completion_date}}</td>
                        <td>{{total_questions}}</td>
                        <td>{{avg_time_per_question}}</td>
                        <td>{{session_time}}</td>
                        <td>{{answered_questions}}</td>
                        <td>{{correct_questions}}</td>
                        <td>
                            <div class="history-percentage-cell">
                                <div class="history-donut-chart {{percentage_class}}"></div>
                                <span class="history-percentage-text">{{percent_correct}}%</span>
                            </div>
                        </td>
                    </tr>
                    {{/history_data}}
                        {{^history_data}}
                        <tr>
                            <td colspan="9" class="no-data">{{no_completed_sessions_found}}</td>
                        </tr>
                        {{/history_data}}
                </tbody>
            </table>
        </div>
    </div>
</div>

<!-- Load JavaScript -->
<script src="javascript/stats.js"></script>

